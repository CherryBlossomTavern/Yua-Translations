# responses.self: [%s, sender]
# responses.towards: [%s, sender] [%s, reciever]
# responses.want: [%s, sender]
# counters.self: [%s, sender] [%s, sentCount]
# counters.towards: [%s, sender] [%s, sentCount] [%s, reciever] [%s, recievedCount]
# counters.want: [%s, sender] [%s, wantCount]

# if (responses.towards) -> sender sent + 1 & receiver received + 1
# if (responses.self) -> send + 1
# if (responses.want) -> sender want + 1

yua.commands.baka.description=User is calling another user a baka
yua.commands.baka.responses.towards=**%s** is calling **%s** a baka!
yua.commands.baka.responses.want=**%s** wants to be called a baka
yua.commands.baka.counters.towards=%s has been called baka %s times and %s has called others baka %s times
yua.commands.baka.counters.want=%s has wanted to be called baka %s times

